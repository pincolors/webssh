{"ast":null,"code":"import { computed } from 'vue';\nimport { useI18n } from 'vue-i18n';\n// 引入 Element Plus 的官方语言包\nimport zhCn from 'element-plus/es/locale/lang/zh-cn';\nimport en from 'element-plus/es/locale/lang/en';\nexport default {\n  name: 'App',\n  setup() {\n    // 获取 i18n 实例\n    const {\n      locale\n    } = useI18n();\n\n    // 创建一个计算属性，根据 i18n 的当前语言，返回对应的 Element Plus 语言包\n    const elLocale = computed(() => {\n      // 注意：这里的判断依据要和你 lang/index.js 里 messages 的 key 一致\n      return locale.value === 'en' ? en : zhCn;\n    });\n    return {\n      elLocale\n    };\n  }\n};","map":{"version":3,"names":["computed","useI18n","zhCn","en","name","setup","locale","elLocale","value"],"sources":["C:\\Users\\Adam\\Desktop\\wssh\\webssh\\frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <el-config-provider :locale=\"elLocale\">\r\n      <router-view />\r\n    </el-config-provider>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useI18n } from 'vue-i18n'\r\n// 引入 Element Plus 的官方语言包\r\nimport zhCn from 'element-plus/es/locale/lang/zh-cn'\r\nimport en from 'element-plus/es/locale/lang/en'\r\n\r\nexport default {\r\n  name: 'App',\r\n  setup() {\r\n    // 获取 i18n 实例\r\n    const { locale } = useI18n()\r\n\r\n    // 创建一个计算属性，根据 i18n 的当前语言，返回对应的 Element Plus 语言包\r\n    const elLocale = computed(() => {\r\n      // 注意：这里的判断依据要和你 lang/index.js 里 messages 的 key 一致\r\n      return locale.value === 'en' ? en : zhCn\r\n    })\r\n\r\n    return {\r\n      elLocale\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AASA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,OAAM,QAAS,UAAS;AACjC;AACA,OAAOC,IAAG,MAAO,mCAAkC;AACnD,OAAOC,EAAC,MAAO,gCAA+B;AAE9C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN;IACA,MAAM;MAAEC;IAAO,IAAIL,OAAO,CAAC;;IAE3B;IACA,MAAMM,QAAO,GAAIP,QAAQ,CAAC,MAAM;MAC9B;MACA,OAAOM,MAAM,CAACE,KAAI,KAAM,IAAG,GAAIL,EAAC,GAAID,IAAG;IACzC,CAAC;IAED,OAAO;MACLK;IACF;EACF;AACF","ignoreList":[]},"metadata":{}